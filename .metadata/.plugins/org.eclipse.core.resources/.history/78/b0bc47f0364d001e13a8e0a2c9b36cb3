package com.goldilocks.dto;

public class Employee {

	private int empId;
	private String name;
	private int age;
	
	
	public int getEmpId() {
		return empId;
	}
	
	public String getName() {
		return name;
	}
	
	
	
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	
	public void setName(String name) {
		this.name = name;
	}

	
	
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String toString() {
		return "empId : "+empId+" empName : "+name+" empAge : "+age;
	}
}
